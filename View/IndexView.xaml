        <!--设置面板动画-->
        <Storyboard x:Key="SetOut">
            <DoubleAnimation Duration="0:0:0.5" To="{c:Binding Path=(ActualWidth)* -MinHeight,ElementName=LeftButtonSetting}"
                             Storyboard.TargetName="SetGrid" Storyboard.TargetProperty="X" AccelerationRatio="0.5"/>
        </Storyboard>
        <Storyboard x:Key="Open">
            <DoubleAnimation Storyboard.TargetName="window_aph" Storyboard.TargetProperty="Offset" Duration="0:0:1" From="0" To="1" AccelerationRatio="0.5"/>
            <ColorAnimation Storyboard.TargetName="window_aph" Storyboard.TargetProperty="Color" Duration="0:0:1" To="#FF000000" AccelerationRatio="0.5"/>
        </Storyboard>
        <Storyboard x:Key="SB_WebOpen">
            <DoubleAnimation Storyboard.TargetName="web_aph" Storyboard.TargetProperty="Offset" Duration="0:0:3" From="0" To="1" AccelerationRatio="0.5"/>
