        </Storyboard>
        <Storyboard x:Key="SB_NavChange">
            <DoubleAnimation Storyboard.TargetName="Nav2_X" Storyboard.TargetProperty="X" Duration="0:0:0.3" From="0" To="{c:Binding Path=-ActualWidth-100, ElementName=Nav2}" AccelerationRatio="0.5"/>
            <DoubleAnimation Storyboard.TargetName="Nav_X" Storyboard.TargetProperty="X" Duration="0:0:0.3" From="{c:Binding Path=ActualWidth+100, ElementName=Nav2}" To="0" AccelerationRatio="0.5"/>
        </Storyboard>
        <Storyboard x:Key="SB_Keyboard">
